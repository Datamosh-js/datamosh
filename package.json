{
    "name": "datamosh",
    "version": "3.0.0",
    "description": "Edit images via buffers.",
    "scripts": {
        "test": "npm run lint && npm run jest",
        "jest": "jest ./tests",
        "lint": "echo \"Linting with Prettier\n\" && npx prettier --check . && echo \"Lint passed\n\"",
        "lint-fix": "echo \"Auto-fixing with Prettier...\" && npx prettier --write .",
        "tsc": "tsc -w lib/*.ts",
        "clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm i"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "keywords": [
        "datamosh",
        "mosh",
        "jpg",
        "png",
        "tiff",
        "bmp",
        "gif",
        "edit",
        "image",
        "buffer",
        "glitch"
    ],
    "author": "Michael Sterpka <michaelsterpka@gmail.com>",
    "contributors": [
        "Tyler Laskey <tlaskey3@gmail.com> (https://github.com/tlaskey)",
        "BonjourInternet (https://github.com/BjrInt)"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/mster/datamosh.git"
    },
    "bugs": {
        "url": "https://github.com/mster/datamosh/issues"
    },
    "main": "index.js",
    "dependencies": {
        "file-type": "^16.5.0",
        "image-decode": "^1.2.2",
        "image-encode": "^1.3.1"
    },
    "devDependencies": {
        "@jest/globals": "^29.5.0",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.3.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4"
    }
}
